<h1>New faculty</h1>

<% form_for(@faculty) do |f| %>
  <%= f.error_messages %>

	<DIV id="nameinfo">
  <p>
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </p>
  <p>
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
  </DIV>
<% end %>

<%= link_to 'Back', faculties_path %>