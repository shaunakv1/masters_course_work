<h1>New station</h1>

<% form_for(@station) do |f| %>
  <%= f.error_messages %>

   <p>
    <%= f.label :id %><br />
    <%= f.text_field :id %>
  </p>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', stations_path %>